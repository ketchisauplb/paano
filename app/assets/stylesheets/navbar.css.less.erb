@import "bootstrap_and_overrides.css.less.erb";

.navbar-inner {
  background: @almost_black;
  padding-top: 10px;
  border: none;
  min-height: 30px;
}

.navbar a{
  color: @grey;
  font-weight: bold;
  -o-transition: @transition_time;
  -ms-transition: @transition_time;
  -moz-transition: @transition_time;
  -webkit-transition: @transition_time;
  transition: @transition_time;
}

.navbar a:hover{
  text-decoration: none;
  color: @white;
}
